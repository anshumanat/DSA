
//approach 1- TLE
class Solution {
  public:
    vector<int> productExceptSelf(vector<int>& arr) {
        // code here
        int n = arr.size();
        vector<int>res(n,1);
        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                if(i!=j){
                    res[i] *= arr[j];
                }
            }
        }
        return res;
    }
};
