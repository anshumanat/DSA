//first approach:
class Solution {
  public:
    vector<int> find(vector<int>& arr, int x) {
        // code here
        int n = arr.size();
        int first = -1, last = -1;
        
        for(int i=0;i<n;i++){
            if(arr[i] !=x){
                continue;
            }
            if(first==-1){
                first = i;
            }
            last = i;
        }
        vector<int>res = {first,last};
        return res;
    }
};
